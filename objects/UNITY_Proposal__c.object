    <webLinks>
        <fullName>UNITY_Send_Quote_to_Customer</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send Quote to Customer</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/apex.js&quot;)}
var qid = &quot;{!UNITY_Proposal__c.Id}&quot;;
var status = &quot;{!UNITY_Proposal__c.UNITY_Proposal_Status__c}&quot;;

if(status == &apos;Quote Finalized&apos;){

var agree = confirm(&quot;Send quote to customer?&quot;);

if(agree){

var ajx = document.createElement(&apos;div&apos;);
ajx.setAttribute(&apos;id&apos;,&apos;ajax-div&apos;);
ajx.style.position = &apos;absolute&apos;;
ajx.style.left = &apos;calc(50% - 16px)&apos;;
ajx.style.top = &apos;calc(50% - 16px)&apos;;
ajx.innerHTML = &apos;&lt;img src=&quot;/img/loading32.gif&quot; alt=&quot;Processing...&quot; width=&quot;24px&quot;/&gt;&apos;;
var c = document.getElementById(&apos;ep&apos;);
c.appendChild(ajx);

var result = sforce.apex.execute(&quot;UNITY_AttachmentHelper_Miner&quot;,&quot;sendAndAttachQuote&quot;,{qid:qid,sendEmail:true});

if(result == &apos;Success&apos;){

var d = document.getElementById(&apos;ajax-div&apos;);
var c = document.getElementById(&apos;ep&apos;);
c.removeChild(d);
alert(&quot;The quote has been sent to the customer.&quot;);
window.location.reload();
}else{

var d = document.getElementById(&apos;ajax-div&apos;);
var c = document.getElementById(&apos;ep&apos;);
c.removeChild(d);
alert(result);

}

}

}else{

alert(&apos;The quote can only be sent to the Customer once it is finalized.&apos;);

}</url>
