    <validationRules>
        <fullName>Restrict_Quote_Creation_to_Completed</fullName>
        <active>true</active>
        <description>REstrict Movement from Quote creation to Completion</description>
        <errorConditionFormula>AND(
ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Proposal Creation&apos;),
ISPICKVAL(UNITY_Proposal_Status__c,&apos;Completed&apos;)
)</errorConditionFormula>
        <errorMessage>Quote cannot be set to Completed as this stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Agent_Cannot_Save_Manager_Statuses</fullName>
        <active>false</active>
        <description>Agent cannot save the proposal record in manager statuses</description>
        <errorConditionFormula>AND ( 
CONTAINS( $User.Id , UNITY_Case__r.OwnerId ), 
OR( 
ISPICKVAL(UNITY_Proposal_Status__c, &apos;Approved by Manager&apos;), 
ISPICKVAL(UNITY_Proposal_Status__c, &apos;Rejected by Manager&apos;), 
ISPICKVAL(UNITY_Proposal_Status__c, &apos;Proposal sent for Manager Approval&apos;) 
) 
)</errorConditionFormula>
        <errorMessage>Cannot Save record with Status: Manager Approved / Rejected. Please change to the appropriate status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Cancelled_Completed_Restrict</fullName>
        <active>true</active>
        <description>Restrict edit of record from completed or cancelled.</description>
        <errorConditionFormula>OR( ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Completed&apos;), 
ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Cancelled&apos;) )</errorConditionFormula>
        <errorMessage>Record cannot be edited after it has been set to Completed or Cancelled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Discount_Edit_Customer_Accept</fullName>
        <active>true</active>
        <description>Discount fields cannot be edited when customer status is Accepted</description>
        <errorConditionFormula>if( 
AND( 
ISPICKVAL(  UNITY_Status__c  ,&apos;Accepted&apos;), 
OR( ISCHANGED( UNITY_Discount__c ),  ISCHANGED(  UNITY_Discount_Amount__c  ) )
) 
, true, false)</errorConditionFormula>
        <errorMessage>The Customer has Accepted the Quote, discount fields cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Discount_Percent_Amount</fullName>
        <active>true</active>
        <description>Ensures that either Amount or % field is used</description>
        <errorConditionFormula>OR ( 
 AND (if(UNITY_Discount__c &gt; 0,true,false), if(UNITY_Discount_Amount__c &gt; 0,true,false) )
)</errorConditionFormula>
        <errorMessage>Please enter value in one of the two fields: Discount % or Discount Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Final_TaxError</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$UserRole.Name != &quot;System Administrator&quot;,
OR(
AND(
NOT(CONTAINS(UNITY_Tax_Process_Result__c,&quot;Success&quot;)),
ISPICKVAL(UNITY_Proposal_Status__c,&quot;Agent Review Complete&quot;)
),
AND(
ISPICKVAL(UNITY_Proposal_Status__c,&quot;Agent Review Complete&quot;),
NOT(UNITY_Tax_Processed__c)
)
)
)</errorConditionFormula>
        <errorMessage>Tax Process has not completed or an error occurred while processing tax. Refresh the page and verify that tax process is successful.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Lock_Discount_Fields</fullName>
        <active>true</active>
        <description>Lock discount fields in sent to customer, approved by manager, completed, cancelled statuses</description>
        <errorConditionFormula>OR ( 
if( 
AND( 
OR (ISPICKVAL(  UNITY_Proposal_Status__c  ,&apos;Sent to Customer&apos;), ISPICKVAL( UNITY_Proposal_Status__c ,&apos;Completed&apos;), ISPICKVAL( UNITY_Proposal_Status__c ,&apos;Cancelled&apos;), ISPICKVAL( UNITY_Proposal_Status__c ,&apos;Returned to Agent&apos;), ISPICKVAL( UNITY_Proposal_Status__c ,&apos;Discount - Approved by Manager&apos;)), 
ISCHANGED( UNITY_Discount__c ) 
) 
, true, false), 
if( 
AND( 
OR (ISPICKVAL(  UNITY_Proposal_Status__c  ,&apos;Sent to Customer&apos;), ISPICKVAL( UNITY_Proposal_Status__c ,&apos;Completed&apos;), ISPICKVAL( UNITY_Proposal_Status__c ,&apos;Cancelled&apos;), ISPICKVAL( UNITY_Proposal_Status__c ,&apos;Returned to Agent&apos;), ISPICKVAL( UNITY_Proposal_Status__c ,&apos;Discount - Approved by Manager&apos;)), 
ISCHANGED( UNITY_Discount_Amount__c ) 
) 
, true, false) 
)</errorConditionFormula>
        <errorMessage>Discount fields cannot be changed in &apos;Discount - Approved by Manager&apos;, &apos;Sent to Customer&apos;, &apos;Completed&apos;, &apos;Returned to Agent&apos; or &apos;Cancelled&apos; statuses</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_No_Reply_De_Escalation</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.Name = &quot;Customer Proposal&quot; &amp;&amp; ISCHANGED(UNITY_Customer_Provided_Quote_No_Reply__c ) &amp;&amp; !UNITY_Customer_Provided_Quote_No_Reply__c &amp;&amp; ISPICKVAL( UNITY_De_Escalate_Reason_No_Reply__c , &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>UNITY_De_Escalate_Reason_No_Reply__c</errorDisplayField>
        <errorMessage>De-Escalation reason must be provided.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Proposal_Restrict_Customer_Edit</fullName>
        <active>true</active>
        <description>Restrict customer from editing the Proposal when it is not in &quot;Sent to Customer&quot; status</description>
        <errorConditionFormula>AND(!ISPICKVAL( UNITY_Proposal_Status__c , &apos;Sent to Customer&apos;) , if(CONTAINS( $Profile.Name , &apos;UNITY_Customer&apos;),true,false) )</errorConditionFormula>
        <errorMessage>The Quote is currently being handled by the Agent. The quote cannot be edited at this moment. Please contact the Agent for further clarification</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Quote_Restrict_Line_Item_add</fullName>
        <active>true</active>
        <description>Restrict line item addition once the proposal status is accepted</description>
        <errorConditionFormula>AND(
ISCHANGED(UNITY_Line_Item_Count__c) ,  ISPICKVAL( UNITY_Status__c ,&quot;Accepted&quot; ) 
)</errorConditionFormula>
        <errorMessage>You may not modify the Quote or Line Items once the Quote is accepted</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Quote_status_restrictions</fullName>
        <active>true</active>
        <description>Disallow illegal status changes</description>
        <errorConditionFormula>AND ( 
RecordType.Name = &apos;Customer Proposal&apos;, 
OR ( 


If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Proposal sent for Manager Approval&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Returned to Agent&apos; )), true, false) , 
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Discount - Approved by Manager&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Returned to Agent&apos; )), true, false) , 
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Completed&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Returned to Agent&apos; )), true, false) , 
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Cancelled&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Returned to Agent&apos; )), true, false) , 

If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Proposal sent for Manager Approval&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Sent to Customer&apos; )), true, false) , 
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Discount - Rejected by Manager&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Sent to Customer&apos; )), true, false) , 
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Discount - Approved by Manager&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Sent to Customer&apos; )), true, false) , 
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Proposal Creation&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Sent to Customer&apos; )), true, false) , 
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Proposal Amended&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Sent to Customer&apos; )), true, false) ,
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Completed&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Sent to Customer&apos; )), true, false) ,
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Cancelled&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Sent to Customer&apos; )), true, false) , 

If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Sent to Customer&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Discount - Rejected by Manager&apos; )), true, false) , 
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Discount - Approved by Manager&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Discount - Rejected by Manager&apos; )), true, false) , 
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Completed&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Discount - Rejected by Manager&apos; )), true, false) , 

If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Proposal sent for Manager Approval&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Discount - Approved by Manager&apos; )), true, false) , 
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Returned to Agent&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Discount - Approved by Manager&apos; )), true, false) , 
If ( AND (ISPICKVAL( UNITY_Proposal_Status__c , &apos;Completed&apos;), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Discount - Approved by Manager&apos; )), true, false) , 

If ( AND ((ISPICKVAL( UNITY_Proposal_Status__c , &apos;Completed&apos;)), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Proposal Creation&apos; )), true, false) ,
If ( AND ((ISPICKVAL( UNITY_Proposal_Status__c , &apos;Discount - Approved by Manager&apos;)), ISPICKVAL(PRIORVALUE( UNITY_Proposal_Status__c ),&apos;Proposal Creation&apos; )), true, false) 

) 
)</errorConditionFormula>
        <errorMessage>This Quote status change is not permitted. Please set the Quote record to a correct status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Reject_reason</fullName>
        <active>false</active>
        <description>Add a reason when customer rejects the quote</description>
        <errorConditionFormula>AND( 
NOT($Profile.Name = &apos;System Administrator&apos;), 
ISPICKVAL( UNITY_Status__c ,&apos;Rejected&apos;) ,
UNITY_Comments__c = &apos;&apos; 
)</errorConditionFormula>
        <errorDisplayField>UNITY_48_Hours__c</errorDisplayField>
        <errorMessage>Please add a reason in the comments section for rejecting this quote</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Restrict_Agents_Skipping_Approvals</fullName>
        <active>true</active>
        <description>If discount exists and Manger Approved Discount is unchecked, the proposal cannot move into following status: Sent to Customer, Return to Agent, Proposal Completed, Proposal Amended &amp; sent to customer.</description>
        <errorConditionFormula>AND
(
OR  
(
 if(UNITY_Discount_Amount__c &gt; 0,true,false),
 if(UNITY_Discount__c &gt; 0,true,false),
 if(UNITY_Line_Item_Discount_Exists__c &gt; 0,true,false),
 if(UNITY_Does_Discount_Amount_Exists__c &gt; 0,true,false)
)
, 
 if(UNITY_Manager_Approved_Discounts__c = false,true,false)
,
OR
(  
 if(ISPICKVAL(UNITY_Proposal_Status__c, &apos;Sent to Customer&apos;),true,false),
 if(ISPICKVAL(UNITY_Proposal_Status__c, &apos;Returned to Agent&apos;),true,false),
 if(ISPICKVAL(UNITY_Proposal_Status__c, &apos;Completed&apos;),true,false)
)
)</errorConditionFormula>
        <errorMessage>The Quote contains discounts. Please submit the Quote for approval. Please contact the system administrator for more details.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Restrict_Proposal_Amended</fullName>
        <active>true</active>
        <description>Cannot move status to Proposal Amended if the Customer status is Accepted</description>
        <errorConditionFormula>AND ( OR (ISPICKVAL( UNITY_Status__c , &apos;Accepted&apos;) , AND ( UNITY_Manager_Approved_Discounts__c  = true, ISPICKVAL(UNITY_Status__c,&apos;&apos;) ) ) , 

OR ( ISPICKVAL( UNITY_Proposal_Status__c , &apos;Proposal Amended&apos;) , ISPICKVAL( UNITY_Proposal_Status__c , &apos;Proposal Creation&apos;) , ISPICKVAL( UNITY_Proposal_Status__c , &apos;&apos;)) 
)</errorConditionFormula>
        <errorMessage>The Quote cannot be amended once it has been Accepted by the customer</errorMessage>
    </validationRules>
